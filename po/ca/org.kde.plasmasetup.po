#, kde-format
msgctxt "@label:textbox"
msgid "Hostname:"
msgstr "Nom de màquina:"

#: modules/hostnameutil/hostnameutil.cpp:94
#, kde-format
msgctxt "@info"
msgid "Hostname cannot be empty."
msgstr "El nom de màquina no pot estar buit."

#: modules/hostnameutil/hostnameutil.cpp:96
#, kde-format
msgctxt "@info"
msgid "Hostname cannot be “localhost” or “localhost.localdomain”."
msgstr "El nom de màquina no pot ser «localhost» ni «localhost.localdomain»."

#: modules/hostnameutil/hostnameutil.cpp:98
#, kde-format
msgctxt "@info"
msgid "Hostname is too long (maximum 253 characters)."
msgstr "El nom de màquina és massa llarg (màxim 253 caràcters)."

#: modules/hostnameutil/hostnameutil.cpp:100
#, kde-format
msgctxt "@info"
msgid "Hostname cannot start with a dot."
msgstr "El nom de màquina no pot començar amb un punt."

#: modules/hostnameutil/hostnameutil.cpp:102
#, kde-format
msgctxt "@info"
msgid "Hostname cannot end with a dot."
msgstr "El nom de màquina no pot acabar amb un punt."

#: modules/hostnameutil/hostnameutil.cpp:104
#, kde-format
msgctxt "@info"
msgid "Hostname cannot contain consecutive dots."
msgstr "El nom de màquina no pot contenir punts consecutius."

#: modules/hostnameutil/hostnameutil.cpp:106
#, kde-format
msgctxt "@info"
msgid "Hostname labels cannot be empty."
msgstr "Les etiquetes del nom de màquina no poden estar buides."

#: modules/hostnameutil/hostnameutil.cpp:108
#, kde-format
msgctxt "@info"
msgid "Each hostname label must be at most 63 characters."
msgstr "Cada etiqueta del nom de màquina pot tenir 63 caràcters com a màxim."

#: modules/hostnameutil/hostnameutil.cpp:110
#, kde-format
msgctxt "@info"
msgid ""
"Hostnames may contain letters, numbers, and hyphens. Each label must start "
"and end with a letter or number."
msgstr ""
"Els noms de màquina poden contenir lletres, nombres i guions. Cada etiqueta "
"ha de començar i acabar amb una lletra o un número."

